{
  "manifest_version": 3,
  "name": "She4",
  "version": "0.0.1",
  "description": "Crazy GF Extension",
  "permissions": ["tabs", "alarms"],
  "host_permissions": ["<all_urls>"],
  "background": { 
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "action": { 
    "default_popup": "index.html" 
  }
}