{
  "manifest_version": 3,
  "name": "She4",
  "version": "1.0",
  "description": "Crazy GF Extension",

  "permissions": ["tabs", "alarms","storage"],

  "host_permissions": [
    "<all_urls>"
  ],

  "background": {
    "service_worker": "background.js"
  },

  "action": {
    "default_popup": "index.html"
  },
 "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "run_at": "document_end"
  }
]
}

